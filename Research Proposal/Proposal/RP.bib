	journal = {The Journal of Finance},
	number = {2},
	pages = {779-823},
	title = {Measuring Innovation and Product Differentiation: Evidence from Mutual Funds},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/jofi.12853},
